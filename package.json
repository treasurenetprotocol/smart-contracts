{
  "name": "contracts",
  "version": "1.0.0",
  "description": "<p align=\"center\">   <a href=\"https://treasurenet.io\">     <img alt=\"treasurenetLogo\" src=\"https://raw.githubusercontent.com/treasurenetprotocol/docs/feature/1.0.3/static/img/logo_tn_github.png\" width=\"250\" />   </a> </p>",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "test": "c8 --reporter=text-summary --reporter=lcov --reports-dir=coverage-js hardhat test",
    "coverage:js": "npm run test",
    "coverage:solidity": "hardhat coverage",
    "node": "hardhat node",
    "ganache": "./ganache/ganache.sh",
    "test:debug": "hardhat test --network ganache --verbose",
    "deploy:base:dev": "bash ./scripts/tasks.sh --network dev --type deploy --steps base producers tcash ustn crosschain",
    "deploy:tokenlocker:dev": "bash ./scripts/tasks.sh --network dev --type deploy --steps tokenlocker",
    "deploy:bid:dev": "bash ./scripts/tasks.sh --network dev --type deploy --steps bid",
    "deploy:base:mainnet": "bash ./scripts/tasks.sh --network mainnet --type deploy --steps base producers tcash ustn crosschain",
    "deploy:tokenlocker:mainnet": "bash ./scripts/tasks.sh --network mainnet --type deploy --steps tokenlocker",
    "deploy:bid:mainnet": "bash ./scripts/tasks.sh --network mainnet --type deploy --steps bid",
    "deploy:base:testnet": "bash ./scripts/tasks.sh --network testnet --type deploy --steps base producers tcash ustn crosschain",
    "deploy:bid:testnet": "bash ./scripts/tasks.sh --network testnet --type deploy --steps bid",
    "deploy:base:dev2": "bash ./scripts/tasks.sh --network dev2 --type deploy --steps base producers tcash ustn crosschain tokenlocker wunit_wtcash",
    "deploy:base:ethereum": "bash ./scripts/tasks.sh --network ethereum --type deploy --steps base producers tcash ustn crosschain tokenlocker wunit_wtcash",
    "deploy:tokenlocker:ethereum": "bash ./scripts/tasks.sh --network ethereum --type deploy --steps tokenlocker",
    "deploy:wunit-wtcash:ethereum": "bash ./scripts/tasks.sh --network ethereum --type deploy --steps crosschain",
    "upgrade:tcashloan:dev": "bash ./scripts/tasks.sh --network dev --type upgrade --steps tcashloan",
    "upgrade:tcash:dev": "bash ./scripts/tasks.sh --network dev --type upgrade --steps tcash",
    "upgrade:tcashauction:dev": "bash ./scripts/tasks.sh --network dev --type upgrade --steps tcashauction",
    "upgrade:tcash:mainnet": "bash ./scripts/tasks.sh --network mainnet --type upgrade --steps tcash",
    "upgrade:wtcash:dev2": "bash ./scripts/tasks.sh --network dev2 --type upgrade --steps wtcash",
    "upgrade:producers:dev": "bash ./scripts/tasks.sh --network dev --type upgrade --steps producers",
    "upgrade:producers:mainnet": "bash ./scripts/tasks.sh --network mainnet --type upgrade --steps producers",
    "upgrade:oracle:mainnet": "bash ./scripts/tasks.sh --network mainnet --type upgrade --steps oracle",
    "lint": "eslint -c './.eslintrc.js' --fix",
    "lint:check": "eslint -c './.eslintrc.js' --ext .js,.cjs --max-warnings=0 test scripts hardhat.config.js",
    "solhint": "./node_modules/.bin/solhint -f table contracts/**/**/*.sol --fix",
    "solhint:check": "./node_modules/.bin/solhint -f table contracts/**/**/*.sol",
    "prettier": "./node_modules/.bin/prettier --write contracts/**/**/*.sol",
    "prettier:check": "./node_modules/.bin/prettier --check \"contracts/**/**/*.sol\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.2",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@nomicfoundation/hardhat-verify": "^2.1.0",
    "@nomiclabs/hardhat-truffle5": "^2.1.0",
    "@openzeppelin/contracts": "^4.9.6",
    "@openzeppelin/contracts-upgradeable": "^4.9.6",
    "@openzeppelin/hardhat-upgrades": "^3.9.1",
    "@openzeppelin/truffle-upgrades": "1.17.0",
    "@openzeppelin/upgrades-core": "1.20.2",
    "@truffle/hdwallet-provider": "^1.7.0",
    "c8": "^10.1.3",
    "chai": "^4.3.7",
    "eslint": "^8.57.0",
    "eslint-config-treasurenet": "^1.0.1",
    "hardhat": "^2.22.15",
    "js-sha3": "^0.8.0",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.16",
    "truffle-assertions": "^0.9.2"
  },
  "dependencies": {
    "@web3-kms-signer/core": "^1.0.16",
    "@web3-kms-signer/kms-provider-aws": "^1.0.6",
    "@web3-kms-signer/kms-wallets": "^1.0.6",
    "asn1.js": "^5.4.1",
    "aws-sdk": "^2.1692.0",
