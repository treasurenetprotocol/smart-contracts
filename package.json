{
  "name": "contracts",
  "version": "1.0.0",
  "description": "<p align=\"center\">   <a href=\"https://treasurenet.io\">     <img alt=\"treasurenetLogo\" src=\"https://raw.githubusercontent.com/treasurenetprotocol/docs/feature/1.0.3/static/img/logo_tn_github.png\" width=\"250\" />   </a> </p>",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "ganache": "./ganache/ganache.sh",
    "test": "truffle test --network ganache",
    "test:debug": "truffle test --network ganache --debug",
    "compile": "truffle compile --all",
    "migrate": "truffle migrate --network ganache",
    "migrate:tn-mainnet": "truffle migrate -f 1 --to 5 --network tn_mainnet",
    "migrate:tn-mainnet-tokenlocker": "truffle migrate -f 6 --to 6 --network tn_mainnet",
    "migrate:treasurenet-tokenlocker": "truffle migrate -f 6 --to 6 --network treasurenet",
    "migrate:treasurenetonlymigrate": "truffle migrate -f 1 --to 5 --network treasurenet",
    "migrate:ethereumonlymigrate": "truffle migrate -f 1 --to 7 --network ethereum",
    "migrate:ethereum-tokenlocker": "truffle migrate -f 6 --to 6 --network ethereum",
    "migrate:ethereum-wunit-wtcash": "truffle migrate -f 7 --to 7 --network ethereum",
    "upgrade:tcashloan:treasurenet": "truffle migrate -f 6 --to 6 --network treasurenet",
    "upgrade:tcash:treasurenet": "truffle migrate -f 14 --to 14 --network treasurenet",
    "upgrade:tcash:tn-mainnet": "truffle migrate -f 14 --to 14 --network tn_mainnet",
    "lint": "eslint -c './.eslintrc.js' --fix",
    "solhint": "./node_modules/.bin/solhint -f table contracts/**/**/*.sol --fix",
    "prettier": "./node_modules/.bin/prettier --write contracts/**/**/*.sol"
  },
  "keywords": [],
  "author": "",
